<h1> Liste des clubs </h1>
<table>
    <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Creation date</th>
        <th>Actions</th>

    </tr>
    {% for c in clubs %}
        <tr>
            <td>{{c.id}}</td>
            <td>{{c.name}}</td>
            <td>{{c.creationDate|date}}</td>
            <td><a href="{{path('club_get',{'id':c.id})}}">Details</a></td>
            <td><a href="{{path('club_remove',{'id':c.id})}}">Remove</a></td>
            <td><a href="{{path('club_update',{'id':c.id})}}">Update</a></td>


        </tr>
    {% endfor %}
</table>